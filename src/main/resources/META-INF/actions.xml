<?xml version="1.0" encoding="UTF-8"?>
<tns:soapui-actions xmlns:tns="http://eviware.com/soapui/config">
    <!-- Created by Kondasamy J
Mail To: Kondasamy@outlook.com-->

    <!--Export Response actions-->
    <tns:action id="TestStepSaveResponseAction" actionClass="com.kondasamy.soapui.plugin.TestStepExportRunResultsAction"/>
    <tns:action id="TestCaseSaveResponseAction" actionClass="com.kondasamy.soapui.plugin.TestCaseExportRunResultsAction"/>
    <tns:action id="TestSuiteSaveResponseAction" actionClass="com.kondasamy.soapui.plugin.TestSuiteExportRunResultsAction"/>

    <tns:actionGroup id="WsdlTestCaseActions">
        <tns:actionMapping actionId="TestCaseSaveResponseAction"/>
    </tns:actionGroup>

    <tns:actionGroup id="TestCaseActions">
        <tns:actionMapping actionId="TestCaseSaveResponseAction"/>
    </tns:actionGroup>

    <tns:actionGroup id="WsdlTestSuiteActions">
        <tns:actionMapping actionId="TestSuiteSaveResponseAction"/>
    </tns:actionGroup>

    <tns:actionGroup id="TestSuiteActions">
        <tns:actionMapping actionId="TestSuiteSaveResponseAction"/>
    </tns:actionGroup>

</tns:soapui-actions>

